            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name="apps.proyecto.pairumani.appbuild.ResultadosActivity" />
        <activity
            android:name="apps.proyecto.pairumani.appbuild.LosaActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name="apps.proyecto.pairumani.appbuild.ResultadoLosaActivity" />
        <activity android:name="apps.proyecto.pairumani.appbuild.RadierActivity" />
        <activity android:name="apps.proyecto.pairumani.appbuild.ResultadoRadierActivity" />
        <activity android:name="apps.proyecto.pairumani.appbuild.CotizarActivity"></activity>
    </application>

</manifest>