        </activity>
        <activity
            android:name="apps.proyecto.pairumani.appbuild.LogInActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name="apps.proyecto.pairumani.appbuild.HomeActivity"
